{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(seller)/addlistingform","src/types/esri-leaflet-geocoder.d.ts", "src/app/HomeDetail/[id].tsx", "src/app/(seller)/sellermiddleware.jsx", "src/app/components/WithoutLogin/OurValue/detail.jsx"],
  "exclude": ["node_modules"]
}
